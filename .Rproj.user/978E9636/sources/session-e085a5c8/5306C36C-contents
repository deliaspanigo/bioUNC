


control_var_selection_anova <- function(var_vr, var_factor){

  dt_output <- FALSE  
  list_control <- list()
  list_control[[1]] <- !is.null(var_vr)
  list_control[[2]] <- !is.null(var_factor)
  list_control[[3]] <- var_vr != ""
  list_control[[4]] <- var_factor != ""
  list_control[[5]] <- var_vr != var_factor
  
}